<div class="page-header">
  <h3>Location Weather #<%= @location_weather.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Location</dt>
      <dd><%= @location_weather.location_id %></dd>

      <dt>Month</dt>
      <dd><%= @location_weather.month %></dd>

      <dt>Temperature</dt>
      <dd><%= @location_weather.temperature %></dd>

      <dt>Weather</dt>
      <dd><%= @location_weather.weather %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/location_weathers" class="btn btn-primary">
        Back
      </a>
      <a href="/location_weathers/<%= @location_weather.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_location_weather/<%= @location_weather.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
